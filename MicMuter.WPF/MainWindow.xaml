<reactiveui:ReactiveWindow
        x:TypeArguments="local:MainWindowViewModel"
        x:Class="MicMuter.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:reactiveui="http://reactiveui.net"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        xmlns:properties="clr-namespace:MicMuter.WPF"
        xmlns:local="clr-namespace:MicMuter.WPF"
        mc:Ignorable="d"
        Title="MicMuter" Height="1" Width="1" Visibility="Collapsed" ShowInTaskbar="False">
    <Grid>
        <tb:TaskbarIcon
            x:Name="TaskbarIcon"
            ToolTipText="hello world">
        </tb:TaskbarIcon>
    </Grid>
</reactiveui:ReactiveWindow>
